めようした //
いうあだ //
きそば //
やしている //
られるような //
えるまで //
したところで //
のみだ //
もあるので //
きから //
りであり //
しかいない //
くなったため //
したものも //
ぴえろ //
もうける //
もなお //
してください //
もそのまま //
かずみ //
つけられる //
からさらに //
しないで //
とりあえず //
けよう //
だんだん //
くべき //
ましく //
かたち //
なっているもの //
まかな //
どっち //
いたるまで //
でやや //
むようになった //
のぞき //
なってきた //
まっていった //
いやり //
げたもの //
ったも //
なものだった //
りだった //
もらい //
しんぶん //
できるような //
とどまらず //
まれつき //
どこまで //
いつしか //
きらり //
づけられ //
りかかった //
けるか //
