using Nemerle.Compiler;

namespace NReporter
{
  /// Этот класс нужен чтобы воспользоваться возможностями компилятора Nemerle
  class CompilerHost : ManagerClass
  {
    public this()
    {
      base(CompilationOptions());
      InitCompiler();
      LoadExternalLibraries();
    }
  }
}
