using System;

namespace NReporter
{
  /// <summary>
  /// Description of LateEx.
  /// </summary>
  module LateEx
  {
    public CreateObject(progId : string) : object
    {
      Threading.Thread.CurrentThread.CurrentCulture = 
        Globalization.CultureInfo("en-US");
      Activator.CreateInstance(Type.GetTypeFromProgID(progId));
    }
  }
}
