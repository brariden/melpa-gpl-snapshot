using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Console;
using System.Linq;

using Nemerle.Imperative;
using Snippets;


module Program
{
  Main() : void
  {
    def clone() {
      WriteLine("clone");
      null
    }
    def dispose() {
      WriteLine("dispose");
    }
    def obj = newobj {
        IDisposable : {dispose};
        ICloneable :  {Clone = clone}
        }
    WriteLine(obj is ICloneable);
    obj.Dispose();
    def _ = (obj :> ICloneable).Clone();
  }
}